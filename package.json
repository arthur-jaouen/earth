{
  "name": "earth",
  "version": "0.0.2",
  "description": "Website to monitor climate and volcanoes from multiple sources",
  "author": "Arthur Jaouen",
  "license": "MIT",
  "repository": "git@github.com:arthur-jaouen/earth.git",
  "homepage": "https://arthur-jaouen.github.io/earth",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --progress --env=dev",
    "dev-quiet": "webpack serve --env=dev > /dev/null 2> /dev/null",
    "build": "webpack",
    "analyze": "webpack --env=analyze",
    "lint": "eslint src --ext .ts,.tsx",
    "fix": "eslint src --ext .ts,.tsx --fix",
    "fmt": "prettier -w .",
    "up": "yarn upgrade --latest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/plugin-transform-runtime": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@babel/runtime": "7.23.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@reduxjs/toolkit": "1.9.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.8.1",
    "dayjs": "1.11.10",
    "eslint": "8.52.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "9.0.0",
    "html-webpack-plugin": "5.5.3",
    "mini-css-extract-plugin": "2.7.6",
    "node-sass": "9.0.0",
    "postcss": "8.4.31",
    "postcss-loader": "7.3.3",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "react-redux": "8.1.3",
    "react-refresh": "0.14.0",
    "react-router-dom": "6.17.0",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "import",
      "jsx-a11y",
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "root": true,
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false
  }
}
